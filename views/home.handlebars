<div class="container-fluid">
  <div class="row">
		  {{#each imgs}}
      <a href="/images/{{this.name}}">
        <div class="col-md-4 col-sm-6 img-holder nopadding">
          <img src="{{this.url}}" type="{{this.type}}">
          <div class="img-tags">
              {{this.uploader}}
          </div>
        </div>
      </a>
      {{/each}}
  </div>
</div>
